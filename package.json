{
  "name": "learn-coding-tutorials",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.1",
    "redux": "^3.7.2",
    "reselect": "^3.0.1"
  },
  "scripts": {
    "build-css": "node-less-chokidar src/style/ src/style",
    "watch-css": "npm run build-css && node-less-chokidar src/style src/style --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "node-less-chokidar": "git+https://github.com/sampi/node-less-chokidar.git",
    "node-sass-chokidar": "^1.1.2",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.1"
  }
}
